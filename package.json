{
    "name": "firecms-parent",
    "private": true,
    "description": "Awesome Firebase/Firestore-based headless open-source CMS",
    "author": "camberi",
    "homepage": "https://firecms.co",
    "workspaces": [
        "lib",
        "example",
        "example-client-project",
        "saas",
        "collection_editor",
        "user_management",
        "data_enhancement"
    ],
    "scripts": {
        "dev": "cd example && yarn dev",
        "saas": "cd saas && yarn dev",
        "website": "cd website && yarn start",
        "preview": "cd example && yarn preview",
        "deploy:saas": "cd saas && yarn deploy:staging",
        "deploy:saas:prod": "cd saas && yarn deploy:prod",
        "deploy:demo": "cd example && yarn deploy",
        "deploy:web": "cd website && yarn deploy",
        "build": "cd lib && yarn build"
    },
    "packageManager": "yarn@3.2.3"
}
